<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.updateAppointmentController">
    <children>
        <Label layoutX="50.0" layoutY="54.0" text="Appointment ID" />
        <Label layoutX="50.0" layoutY="94.0" text="Title" />
        <Label layoutX="50.0" layoutY="134.0" text="Description" />
        <Label layoutX="50.0" layoutY="174.0" text="Location" />
        <Label layoutX="50.0" layoutY="214.0" text="Contact" />
        <Label layoutX="50.0" layoutY="254.0" text="Type" />
        <Label layoutX="50.0" layoutY="294.0" text="Customer" />
        <Label layoutX="353.0" layoutY="137.0" text="End Time" />
        <Label layoutX="351.0" layoutY="101.0" text="Start Time" />
        <Label layoutX="50.0" layoutY="334.0" text="Scheduled By" />
        <TextField fx:id="idBox" editable="false" layoutX="180.0" layoutY="50.0" />
        <TextField fx:id="titleBox" layoutX="180.0" layoutY="90.0" />
        <TextField fx:id="descBox" layoutX="180.0" layoutY="130.0" />
        <TextField fx:id="locBox" layoutX="180.0" layoutY="170.0" />
        <ComboBox fx:id="contactDrop" layoutX="180.0" layoutY="210.0" prefWidth="150.0" />
        <TextField fx:id="typeBox" layoutX="180.0" layoutY="250.0" />
        <ComboBox fx:id="customerDrop" layoutX="180.0" layoutY="290.0" onAction="#custSelected" prefWidth="150.0" />
        <DatePicker fx:id="dateBox" layoutX="412.0" layoutY="58.0" onAction="#datePicked" promptText="Select Date" />
        <ComboBox fx:id="startDrop" layoutX="412.0" layoutY="97.0" onAction="#timePicked" prefHeight="25.0" prefWidth="86.0" />
        <ComboBox fx:id="endDrop" layoutX="412.0" layoutY="133.0" onAction="#lengthPicked" prefHeight="25.0" prefWidth="86.0" />
        <ComboBox fx:id="userDrop" layoutX="180.0" layoutY="330.0" prefWidth="150.0" />
        <Label layoutX="378.0" layoutY="62.0" text="Date" />
        <Button layoutX="416.0" layoutY="290.0" mnemonicParsing="false" onAction="#saveAndExitPressed" text="Save and Exit" />
        <Button fx:id="exitButton" layoutX="429.0" layoutY="330.0" mnemonicParsing="false" onAction="#exitPressed" text="Go Back" />
        <Label layoutX="530.0" layoutY="101.0" text="Duration" />
        <Label fx:id="durText" layoutX="516.0" layoutY="137.0" />
    </children>
</AnchorPane>
